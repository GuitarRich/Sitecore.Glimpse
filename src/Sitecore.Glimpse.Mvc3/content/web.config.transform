<configuration>
  <glimpse defaultRuntimePolicy="On" endpointBaseUri="~/Glimpse.axd">
    <tabs>
      <ignoredTypes>
        <add type="Glimpse.AspNet.Tab.Routes, Glimpse.AspNet" />
      </ignoredTypes>
    </tabs>
    <inspectors>
      <ignoredTypes>
        <add type="Glimpse.AspNet.Inspector.RoutesInspector, Glimpse.AspNet" />
      </ignoredTypes>
    </inspectors>
  </glimpse>
</configuration>